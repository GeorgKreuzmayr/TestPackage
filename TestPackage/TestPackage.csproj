        <PackageReference Include="Equ" Version="2.2.0" />
        <PackageReference Include="JetBrains.Annotations" Version="2019.1.3" />
        <PackageReference Include="Mono.Cecil" Version="0.11.0" />
    </ItemGroup>

    <ItemGroup>
        <None Remove="License.md" />
    </ItemGroup>

</Project>
